#include <stdio.h>

int main() {
    int decimal, octal = 0, i = 1, remainder;

    printf("Enter a decimal number: ");
    scanf("%d", &decimal);

    int temp = decimal;

    // Convert decimal to octal
    while(temp != 0) {
        remainder = temp % 8;
        octal += remainder * i;
        temp /= 8;
        i *= 10;
    }

    printf("Octal equivalent: %d\n", octal);

    return 0;
}
