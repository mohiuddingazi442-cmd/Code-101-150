#include <stdio.h>

struct Distance {
    int feet;
    int inch;
};

int main() {
    struct Distance d[100];
    int n, i;
    int totalFeet = 0, totalInch = 0;

    printf("Enter number of distances: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("\nEnter distance %d\n", i + 1);
        printf("Feet: ");
        scanf("%d", &d[i].feet);
        printf("Inch: ");
        scanf("%d", &d[i].inch);

        totalFeet += d[i].feet;
        totalInch += d[i].inch;
    }

    /* Convert inches to feet */
    totalFeet += totalInch / 12;
    totalInch = totalInch % 12;

    printf("\nTotal Distance = %d feet %d inch\n",
           totalFeet, totalInch);

    return 0;
}
