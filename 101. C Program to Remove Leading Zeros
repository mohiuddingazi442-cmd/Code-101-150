#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i = 0;

    printf("Enter a string (number with leading zeros): ");
    scanf("%s", str);

    // Skip all leading zeros
    while (str[i] == '0') {
        i++;
    }

    // If all characters were zeros, print a single zero
    if (i == strlen(str)) {
        printf("0\n");
    } else {
        printf("%s\n", str + i);
    }

    return 0;
}
